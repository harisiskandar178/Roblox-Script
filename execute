    -- Scripts:
    
    local function AOVN_fake_script() -- executetab.LocalScript 
        local script = Instance.new('LocalScript', executetab)
    
        script.Parent.MouseButton1Click:Connect(function()
            script.Parent.Parent.Parent["executor frame"].Visible = true
            script.Parent.Parent.Parent["ss frame"].Visible = false
            script.Parent.Parent.Parent["script frame"].Visible = false
            script.Parent.Parent.Parent["search script"].Visible = false
        end)
    end
    coroutine.wrap(AOVN_fake_script)()
    local function TKTYS_fake_script() -- scripttab.LocalScript 
        local script = Instance.new('LocalScript', scripttab)
    
        script.Parent.MouseButton1Click:Connect(function()
            script.Parent.Parent.Parent["executor frame"].Visible = false
            script.Parent.Parent.Parent["ss frame"].Visible = false
            script.Parent.Parent.Parent["script frame"].Visible = true
            script.Parent.Parent.Parent["search script"].Visible = true
        end)
    end
    coroutine.wrap(TKTYS_fake_script)()
    local function RISPF_fake_script() -- sstab.LocalScript 
        local script = Instance.new('LocalScript', sstab)
    
        script.Parent.MouseButton1Click:Connect(function()
            script.Parent.Parent.Parent["executor frame"].Visible = false
            script.Parent.Parent.Parent["ss frame"].Visible = true
            script.Parent.Parent.Parent["script frame"].Visible = false
            script.Parent.Parent.Parent["search script"].Visible = false
        end)
    end
    coroutine.wrap(RISPF_fake_script)()
    local function MGKIIYT_fake_script() -- scriptframe.LocalScript 
        local script = Instance.new('LocalScript', scriptframe)
    
        local scroll = script.Parent.Parent["script frame"]
        local boxsearch = script.Parent.Parent["search script"]
        
        boxsearch.Changed:Connect(function()
            local text = boxsearch.Text:lower()
            if text ~= "" then
                local buttons = scroll:GetDescendants()
                for i,v in pairs(buttons) do
                    if v:IsA("TextButton") then
                        local buttontext = v.Text:lower()
                        if string.find(buttontext, text) then
                            v.Visible = true
                        else
                            v.Visible = false
                        end
                    end
                end
            else
                local buttons = scroll:GetDescendants()
                for i,v in pairs(buttons) do
                    if v:IsA("TextButton") then
                            v.Visible = true
                        end
                    end
            end
        end)
    end
    coroutine.wrap(MGKIIYT_fake_script)()
    local function YUGGCRR_fake_script() -- main.LocalScript 
        local script = Instance.new('LocalScript', main)
    
        local UserInputService,gui,dragging,dragInput,dragStart,startPos = game:GetService("UserInputService"),script.Parent
        local function update(input)
            local delta = input.Position - dragStart
            gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
        gui.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = gui.Position
        
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)
        gui.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                dragInput = input
            end
        end)
        UserInputService.InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end)
    end
    coroutine.wrap(YUGGCRR_fake_script)()

    _G.Frame = {}
    function _G.Frame:Button(Name,Call)
        local scriptbutton = Instance.new("TextButton")

        scriptbutton.Name = "script button"
    scriptbutton.Parent = scriptframe
    scriptbutton.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
    scriptbutton.BorderSizePixel = 0
    scriptbutton.Position = UDim2.new(0.204615384, 0, 0, 0)
    scriptbutton.Size = UDim2.new(0, 250, 0, 50)
    scriptbutton.Font = Enum.Font.GothamBold
    scriptbutton.Text = Name
    scriptbutton.TextColor3 = Color3.fromRGB(0, 0, 0)
    scriptbutton.TextSize = 16.000
    scriptbutton.TextWrapped = true scriptbutton.MouseButton1Click:Connect(function()
        pcall(Call)
    end)
    end
    return _G.Frame
end
return _G.Main
